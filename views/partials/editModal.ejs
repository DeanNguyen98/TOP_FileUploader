<dialog style="
width: 100%;
height: 100%;
position: fixed;
top: 0;
left: 0;
background: rgba(0, 0, 0, 0.4);
" id="editDialog">
    <form method="POST" action="/user/editFolder/<%=id%>">
        <input type="text" id="folderName" name="folderName">
        <button type="submit" id="closeDialog" onclick="closeDialog()">Save</button>
    </form>
 
</dialog>